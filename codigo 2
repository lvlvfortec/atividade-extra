package model;

public class ContaSalario {
    private String nome;
    private String setor;
    private double salario;

    public ContaSalario(String nome, String setor, double salario) {
        this.nome = nome;
        this.setor = setor;
        this.salario = salario;
    }

    public String getNome() {
        return nome;
    }

    public String getSetor() {
        return setor;
    }

    public double getSalario() {
        return salario;
    }
}
